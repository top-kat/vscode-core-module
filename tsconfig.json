	"compilerOptions": {
		"module": "commonjs",
		"lib": [
			"es6"
		],
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"allowJs": true,
		"target": "ES2018",
		"noImplicitAny": false,
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "dist",
		"baseUrl": ".",
	}
}